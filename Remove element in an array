class Solution 
{
    public int removeElement(int[] nums, int val) 
    {
        int m = 0;    
        for(int i = 0; i < nums.length; i++)
        {
       //so if the two values that you are comparing are not the same then move it to the other position
       if(nums[i] != val)
       {
           nums[m] = nums[i];
           m++;
       }
        }
   
   return m;
}
}
